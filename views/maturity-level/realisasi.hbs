<!-- ### Screen Content ### -->
<main class='main-content bgc-grey-100'>
  <div id='mainContent'>
    <div class="row gap-20">
      <div class="col-md-8">
        <h3 class="c-grey-900 mB-15">Input Data Maturity Level</h3>
        <form method="POST" enctype="multipart/form-data">
          {{!-- General --}}
          <div class="bgc-white bd bdrs-3 p-20">
            <div class="form-group row">
              <label for="semester" class="col-sm-4 col-form-label">Semester</label>
              <div class="col-sm-8">
                <select class="form-control" id="semester" required>
                  <option selected="selected">Pilih semester...</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                </select>
              </div>
            </div>
            <div class="form-group row">
              <label for="tahunMaturity" class="col-sm-4 col-form-label">Tahun</label>
              <div class="col-sm-8">
                <div id="datepickerTahun" class="input-group date" data-date-format="yyyy">
                  <input id="tahun" class="form-control" type="text" readonly required/>
                  <div class="input-group-append">
                    <span class="input-group-text input-group-addon"><i class="ti-calendar"></i></span>
                  </div>
                </div>
              </div>
            </div>
            <div class="form-group row">
              <label for="upk" class="col-sm-4 col-form-label">Unit Pelaksana Pembangkit (UPK)</label>
              <div class="col-sm-8">
                <select name="upk" id="upk" class="form-control" required>
                  <option disabled hidden selected>Pilih UPK...</option>
                  {{#each units}}
                  <option value="{{this.username}}">{{this.name}}</option>
                  {{/each}}
                </select>
              </div>
            </div>
          </div>
          {{!-- Pengumpulan Data Efisiensi --}}
          <div class="bgc-white bd bdrs-3 p-20 mT-20">
            <div class="form-group row">
              <div class="col-sm-12">
                <h4 class="font-weight-bolder">Pengumpulan Data Efisiensi</h4>
                <h5>Level 1 (Fire Fighting)</h5>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat PIC yang bertanggung jawab melakukan data collection</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[0][0]" id="maturity-a11-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a11-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[0][0]" id="maturity-a11-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a11-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat sheet data collection ketika melakukan analisa efisiensi</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[0][1]" id="maturity-a12-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a12-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[0][1]" id="maturity-a12-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a12-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12">
                <h5>Level 2 (Stabilizing)</h5>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat data referensi yang sudah di rekap dengan baik (sumber data referensi
                  dari : data commisioning, manual book / Heat Balance Diagram)</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][0]" id="maturity-a21-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a21-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][0]" id="maturity-a21-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a21-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat daftar parameter operasi terkait efisiensi dan terdapat range normal
                  operasi</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][1]" id="maturity-a22-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a22-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][1]" id="maturity-a22-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a22-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat jadwal dan realisasi data collection minimal 80% dari rencana kecuali
                  jika terdapat unplanned outage</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][2]" id="maturity-a23-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a23-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[1][2]" id="maturity-a23-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a23-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12">
                <h5>Level 3 (Preventing)</h5>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat Tindak Lanjut Ketidaksesuaian Data Operasi ( ILS / SR )</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[2][0]" id="maturity-a31-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a31-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[2][0]" id="maturity-a31-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a31-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat hasil tindak lanjut ILS ketidaksesuain data operasi (WO Rise)</span>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[2][1]" id="maturity-a32-ya" value="y" required>
                  <label class="form-check-label" for="maturity-a32-ya">Ya</label>
                </div>
                <div class="form-check form-check-inline">
                  <input class="form-check-input" type="radio" name="pengumpulanDataEfisiensi[2][1]" id="maturity-a32-tidak" value="t" required>
                  <label class="form-check-label" for="maturity-a32-tidak">Tidak</label>
                </div>
              </div>
              <div class="col-sm-12 mB-15">
                <span class="d-block">Terdapat hasil kalibrasi parameter utama terkait efisiensi yang terdokumentasi
